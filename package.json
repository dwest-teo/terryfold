{
  "name": "terryfold",
  "version": "1.0.0",
  "description": "Plays Terryfold",
  "author": "Donny West",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "next",
    "build": "next build && next export",
    "start": "next start"
  },
  "dependencies": {
    "next": "^6.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-ga": "^2.5.0",
    "react-youtube": "^7.6.0"
  },
  "devDependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "eslint-config-xo-react": "^0.16.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.71.0",
    "xo": "^0.20.3"
  },
  "engines": {
    "node": "9.2.0"
  },
  "xo": {
    "envs": [
      "node",
      "es6",
      "browser"
    ],
    "extends": "xo-react",
    "space": true,
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "react/jsx-closing-bracket-location": [
        2,
        "tag-aligned"
      ],
      "react/jsx-tag-spacing": [
        2,
        {
          "closingSlash": "never",
          "beforeSelfClosing": "always",
          "afterOpening": "never"
        }
      ],
      "react/forbid-component-props": 0,
      "capitalized-comments": 0
    }
  }
}
